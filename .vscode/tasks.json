{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "build",
			"command": "gcc",
			"args": [
				"main.c",
				"Lexer.c",
				"-o",
				"bearscript.exe"
			],
			"options": {
				"cwd": "${workspaceFolder}/bearscript/src"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			],
			"presentation": {
				"reveal": "silent"
			}
		}
	]
}